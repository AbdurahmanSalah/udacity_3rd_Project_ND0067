{
  "scripts": {
    "frontend:install": "cd udagram-frontend && npm i",
    "frontend:build": "cd udagram-frontend && npm run build",
    "frontend:deploy": "cd udagram-frontend && npm run deploy",
    "frontend:test": "cd udagram-frontend && npm run test",
    "backend:install": "cd udagram-api && npm i",
    "backend:build": "cd udagram-api && npm run build && npm run archive",
    "backend:deploy": "cd udagram-api && npm run deploy",
    "backend:test": "cd udagram-api && npm run test",
    "install": "npm-run-all *:install",
    "build": "npm-run-all *:build",
    "test": "npm run frontend:test"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
